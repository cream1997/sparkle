{
  "name": "sparkle-cli",
  "private": true,
  "version": "0.0.1",
  "main": "build/main/main.js",
  "author": "cream",
  "scripts": {
    "postinstall": "patch-package && electron-builder install-app-deps",
    "dev": "chcp 65001 && vite",
    "dev2": "chcp 65001 && vite 2>&1 | node ./filter-log.js",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "fix": "eslint --fix .",
    "format": "prettier . --write",
    "generate-icons": "electron-icon-builder --input=./public/icon.png --output=./build/ --flatten",
    "build:unpack": "npm run build && npm run generate-icons && electron-builder --dir",
    "build:win": "npm run build && npm run generate-icons && electron-builder --win"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.8.4",
    "electron-store": "^10.0.1",
    "iconv-lite": "^0.6.3",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "pinia": "^3.0.1",
    "ssh2": "^1.16.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^22.13.11",
    "@types/ssh2": "^1.15.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^35.0.3",
    "electron-builder": "^25.1.8",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "patch-package": "^8.0.0",
    "prettier": "3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^6.2.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.4"
  }
}
