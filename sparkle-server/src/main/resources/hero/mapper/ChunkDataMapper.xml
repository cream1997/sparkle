<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cream.sparkle.mapper.ChunkDataMapper">
    <insert id="insertChunkData">
        insert into ${tableName}(id, data)
        values (#{id}, #{data})
    </insert>

    <update id="updateChunkData">
        update ${tableName}
        set data = #{data}
        where id = #{id}
    </update>

    <select id="selectChunkData" resultType="com.cream.sparkle.object.entity.ChunkData">
        select data
        from ${tableName}
        where id = #{id}
    </select>
</mapper>